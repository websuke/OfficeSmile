<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.officesmile.mapper.userRole.UserRoleMapper">
    <!-- 特定のユーザーをIDで検索するSELECT文 -->
    <insert id="store" parameterType="com.example.officesmile.domain.entity.userRole.UserRoleEntity">
        INSERT INTO user_role (
            user_id,
            role_id
        ) VALUES (
            #{userId},
            #{roleId}
        )
    </insert>
</mapper>
